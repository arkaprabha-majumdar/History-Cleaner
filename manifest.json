{
    "manifest_version": 3,
    "name": "History Cleaner",
    "version": "1.0",
    "description": "Delete sites from history based on input.",
    "permissions": [
      "history"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  